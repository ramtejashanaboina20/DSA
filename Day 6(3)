import java.util.Scanner;
class Main{
    static boolean checkElement(int[] arr,int target){
        for (int i=0; i<arr.length-1; i++){
            if(arr[i]== target){
                return true;
                
            }
        }
        return false;
        }
        public static void main(String[]args){
            int[]arr= {1,2,3,4,5};
            int target = 3;
            System.out.println(checkElement(arr,target));
            }
}
