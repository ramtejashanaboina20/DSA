import java.util.*;

class Main {
    public static void main(String[] args) {
                Scanner in = new Scanner(System.in);

        int[] arr = {1, 2, 3, 4, 5};

        int highestValue = 0;

        for (int i = 0; i < arr.length; i++) {
            if (arr[i] > highestValue) {
                 highestValue = arr[i] ;    

            }
        }

        System.out.println(highestValue);
    }
}

